{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/webhook/:path*",
            "destination": "/api/webhook/:path*"
        },
        {
            "source": "/",
            "destination": "/public/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/public/:path*"
        }
    ]
}